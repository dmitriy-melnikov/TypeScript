{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "semicolon": false,
        "trailing-comma": false,
        "no-console": [false, "log", "error"],
        "no-invalid-this": true,
        "no-shadowed-variable": false,
        "new-parens": true,
        "strictNullChecks": true,
        "strictPropertyInitialization": true,
        "max-classes-per-file": false
    },
    "rulesDirectory": []
}
